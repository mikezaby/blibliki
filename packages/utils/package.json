    "bump": "npm version patch",
    "release": "pnpm run build && pnpm publish --access public"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.10.2"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "deepmerge-ts": "^7.1.5",
    "es-toolkit": "^1.41.0",
    "node-web-audio-api": "1.0.7"

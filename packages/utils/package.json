{
  "name": "@blibliki/utils",
  "version": "0.4.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./web-audio-api": {
      "types": "./dist/web-audio-api.d.ts",
      "import": "./dist/web-audio-api.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "README.md",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",

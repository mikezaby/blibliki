{
  "name": "@blibliki/utils",
  "version": "0.3.10",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./web-audio-api": {
      "types": "./dist/web-audio-api.d.ts",
      "import": "./dist/web-audio-api.js",
      "require": "./dist/web-audio-api.cjs"
    }
  },
  "files": [
    "README.md",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "tsc": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "bump": "npm version patch",
    "release": "pnpm run build && pnpm publish --access public"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.10.2"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "deepmerge-ts": "^7.1.5",
    "es-toolkit": "^1.41.0"
  },
  "optionalDependencies": {
    "node-web-audio-api": "^0.9.3"
  }
}

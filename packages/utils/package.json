{
  "name": "@blibliki/utils",
  "version": "0.5.2",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/index.browser.d.ts",
        "import": "./dist/index.browser.js"
      },
      "node": {
        "types": "./dist/index.node.d.ts",
        "import": "./dist/index.node.js"
      },
      "default": {
        "types": "./dist/index.browser.d.ts",
        "import": "./dist/index.browser.js"
      }
    },
    "./web-audio-api": {
      "browser": {
        "types": "./dist/web-audio-api.browser.d.ts",
        "import": "./dist/web-audio-api.browser.js"
      },
      "node": {
        "types": "./dist/web-audio-api.node.d.ts",
        "import": "./dist/web-audio-api.node.js"
      },
      "default": {
        "types": "./dist/web-audio-api.browser.d.ts",
        "import": "./dist/web-audio-api.browser.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "README.md",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "tsc": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "bump": "npm version patch",
    "release": "pnpm run build && pnpm publish --access public"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.10.2"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "deepmerge-ts": "^7.1.5",
    "es-toolkit": "^1.41.0",
    "node-web-audio-api": "1.0.7"
  }
}

{
  "name": "@blibliki/engine",
  "version": "0.3.10",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",

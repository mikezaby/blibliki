    "release": "pnpm run build && pnpm publish --access public"
  },
  "devDependencies": {
    "@types/audioworklet": "^0.0.91",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@blibliki/transport": "workspace:^",
    "@blibliki/utils": "workspace:^",
    "@julusian/midi": "^3.6.1",
    "es-toolkit": "^1.41.0",
    "node-web-audio-api": "^1.0.3"
  }
}
